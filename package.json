{
  "name": "backendminimal",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "main": "server.js",
  "scripts": {
    "start:dev": "nodemon -r dotenv/config server.js dotenv_config_path=./.env.development.local",
